    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
  mst部署マスタ.部署記号
  ,mst部署マスタ.ソート順
  ,mst部署マスタ.部署形態
  ,mst部署マスタ.本支店区分
