  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@期年度">
            <Value>=Parameters!期年度.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  tbl売上明細テーブル.案件NO
  ,tbl案件テーブル.客先コード
  ,mst客先マスタ.客先名
  ,tbl案件テーブル.案件名
  ,tbl売上テーブル.請求書NO
  ,tbl売上テーブル.請求書枝番
  ,datename(yyyy,tbl売上テーブル.売上計上日)+'/'+datename(mm,tbl売上テーブル.売上計上日) as 売上年月
 ,'期' = CASE 
         WHEN month(tbl売上テーブル.売上計上日) &gt; 9 THEN (year(tbl売上テーブル.売上計上日)-1964)
         ELSE (year(tbl売上テーブル.売上計上日)-1965)
   END 
,'QX' = CASE 
     WHEN month(tbl売上テーブル.売上計上日) &gt; 9 and month(tbl売上テーブル.売上計上日) &lt; 13 THEN 'Q1'
     WHEN month(tbl売上テーブル.売上計上日) &gt; 0 and month(tbl売上テーブル.売上計上日) &lt; 4  THEN 'Q2'
     WHEN month(tbl売上テーブル.売上計上日) &gt; 3 and month(tbl売上テーブル.売上計上日) &lt; 7  THEN 'Q3'
     WHEN month(tbl売上テーブル.売上計上日) &gt; 6 and month(tbl売上テーブル.売上計上日) &lt; 10 THEN 'Q4' 
     END  
,tbl売上テーブル.件名
  ,tbl売上テーブル.請求金額
  ,tbl売上テーブル.現金金額+tbl売上テーブル.手形金額 as 売上金額
  ,tbl売上テーブル.部署記号
  ,tbl案件テーブル.担当者コード
  ,tbl案件テーブル.担当者名
  ,mst部署マスタ.ソート順
  ,mst客先マスタ.特約店FLG
  ,mst客先マスタ.業種コード
 

FROM  tbl売上テーブル 

INNER JOIN tbl売上明細テーブル
    ON tbl売上テーブル.請求書NO = tbl売上明細テーブル.請求書NO and tbl売上テーブル.請求書枝番 = tbl売上明細テーブル.請求書枝番
INNER JOIN tbl案件テーブル
    ON tbl案件テーブル.案件NO = tbl売上明細テーブル.案件NO
INNER JOIN mst部署マスタ
    ON tbl売上テーブル.部署記号 = mst部署マスタ.部署記号
INNER JOIN mst客先マスタ
    ON tbl売上テーブル.客先コード = mst客先マスタ.客先コード AND tbl売上テーブル.支店コード = mst客先マスタ.支店コード

WHERE
 tbl売上テーブル.売上承認 = '2'
 AND mst部署マスタ.部署形態 ='1'
 AND mst客先マスタ.特約店FLG = '1'
 AND tbl売上明細テーブル.行番 = 0
 AND CASE 
         WHEN month(tbl売上テーブル.売上計上日) &gt; 9 THEN (year(tbl売上テーブル.売上計上日)-1964)
         ELSE (year(tbl売上テーブル.売上計上日)-1965)
         END in (@期年度)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
