        <Field Name="説明">
          <DataField>説明</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
  mst管理マスタ.管理コード
  ,mst管理マスタ.管理データ
  ,mst管理マスタ.説明
FROM
  mst管理マスタ
WHERE
  mst管理マスタ.管理コード LIKE N'0002'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
